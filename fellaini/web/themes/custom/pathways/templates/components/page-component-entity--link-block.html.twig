
{#
/**
 * @file
 * basic full width block with plain text links.

 * - logged_in: Flag for authenticated user status. Will be true when the
 *   current user is a logged-in member.
 * - is_admin: Flag for admin user status. Will be true when the current user
 *   is an administrator.

 */
#}
{% set group_modifiers = ['links'] %}

{% set items = content.field_link %}
{% set items_length = items|length %}

{% if items.1 %}
	{% set multiple = true %}
{% endif %}

{% embed "@organisms/grouping/_grouping.twig" with{
  items: items,
  title: content.field_block_title
} %}{% endembed %}
