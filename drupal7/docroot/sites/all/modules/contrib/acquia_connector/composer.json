{
    "name": "drupal/acquia_connector",
    "type": "drupal-module",
    "description": "Allows Drupal websites to connect with Acquia.",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "^5.6 || ^7.1",
        "ext-json": "*"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
        "drupal/coder": "^8.3"
    },
    "extra": {
        "phpcodesniffer-search-depth": 4
    },
    "config": {
        "platform": {
            "php": "5.6.40"
        }
    }
}
